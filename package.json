{
  "name": "sprintus",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.6.3",
    "@nuxtjs/axios": "^5.0.0",
    "@nuxtjs/dotenv": "^1.3.0",
    "cookie-universal-nuxt": "^2.0.11",
    "firebase": "^5.6.0",
    "firebase-admin": "^6.4.0",
    "firebase-functions": "^2.1.0",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "nuxt": "^2.3.4",
    "nuxt-property-decorator": "^1.2.0",
    "spectre.css": "^0.5.7",
    "vuex-class": "^0.3.0",
    "vuexfire": "^3.0.0-alpha.10"
  },
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build && npm run build:copy:ssr",
    "build:copy:ssr": "rimraf functions/ssr && mkdirp functions/ssr && cp -R .nuxt/dist functions/ssr/dist",
    "serve": "firebase serve",
    "deploy": "firebase deploy",
    "start": "HOST=0.0.0.0 nuxt",
    "generate": "nuxt generate"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix",
      "git add"
    ],
    "*.{styl,stylus}": [
      "stylint --config .stylintrc",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@types/lodash": "^4.14.119",
    "@types/node": "^10.12.10",
    "commitizen": "^3.0.5",
    "cz-customizable": "^5.3.0",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-typescript": "^1.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-json": "^1.3.2",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-typescript": "^0.14.0",
    "eslint-plugin-vue": "^4.7.1",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "node-sass": "^4.11.0",
    "nuxt-sass-resources-loader": "^2.0.5",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "^7.1.0",
    "ts-loader": "^5.3.1",
    "typescript": "^3.2.1",
    "typescript-eslint-parser": "^21.0.1",
    "vue-eslint-parser": "^4.0.3"
  }
}
